,b	plugin/command.vim	/^vmap ,b di**<Esc>pa**$/;"	m
,c	plugin/command.vim	/^vmap ,c di`<Esc>pa`$/;"	m
,i	plugin/command.vim	/^vmap ,i di*<Esc>pa*$/;"	m
,u	plugin/command.vim	/^vmap ,u di_<Esc>pa_$/;"	m
